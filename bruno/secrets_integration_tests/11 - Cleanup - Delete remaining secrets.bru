meta {
  name: 11 - Cleanup - Delete remaining secrets
  type: http
  seq: 11
}

delete {
  url: {{burl}}/api/secrets?key={{aws_secret_key}}
  body: none
  auth: inherit
}

params:query {
  key: {{aws_secret_key}}
}

headers {
