meta {
  name: 13 - Cleanup - Delete API token
  type: http
  seq: 13
}

delete {
  url: {{burl}}/api/tokens/{{api_token_id}}
  body: none
  auth: inherit
}

headers {
  Authorization: Bearer {{admin_token}}
}

assert {
  res.status: eq 200
  res.body.success: eq true
}

tests {
  test("Token cleanup should succeed", function() {
    expect(res.getStatus()).to.equal(200);
  });
}
